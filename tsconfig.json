{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es2017",
    "module": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "types": ["vite/client", "@types/node"]
  }
}
